<div class = "content">

  <table class="pure-table pure-table-horizontal">
    <thead>
      <tr>
      <!--   <th>VIN:</th> -->
        <th>Build (#/586):</th>
        <th>Date:</th>
        <th>Exterior:</th>
        <th>Interior:</th>
        <th>Transmission:</th>
      </tr>
    </thead>

    <tbody>
      
    {{#each objects}}

      <tr>
    <!--     <td>{{this.shortVIN}}</td> -->
        <td>{{this.buildNo}}</td>
        <td>{{this.date}}</td>
        <td>{{this.extColor}}</td>
        <td>{{this.intColor}}</td>
        <td>{{this.trans}}</td>
      </tr>
    {{/each}}

    </tbody>
  </table>
</div>
